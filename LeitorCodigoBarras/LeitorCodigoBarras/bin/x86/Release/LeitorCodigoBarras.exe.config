<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="LeitorCodigoBarras.My.MySettings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>

  <appSettings>

    <!--CONFIGURAÇÃO IMAGEM SCANNER-->
    <add key="scanResolutionDPI" value="200"/>
    <add key="scanStartLeftPixel" value="0"/>
    <add key="scanStartTopPixel" value="0"/>
    <add key="scanWidthPixels" value="1420"/>
    <add key="scanHeightPixels" value="550"/>
    <add key="brightnessPercents" value="0"/>
    <add key="contrastPercents" value="0"/>

    <!--QUALIADE IMAGEM: 1 à 100-->
    <add key="qualidadeImagem" value="30"/>

    <!--DIRETORIO RAIZ IMAGENS-->
    <add key="path" value="30"/>


    <!--TRATAMENTO EXCEÇÕES -->
    <!-- Leitura código Barras
            0: Interromper Lote
            1: Perguntar por nova tentativa de leitura
            2: Ignorar
    -->
    <add key="falhaLeituraCodigoBarras" value="1"/>

    <!-- Leitura duplicidade
            0: Interromper Lote
            1: Perguntar por nova tentativa de leitura
            2: Ignorar
            3: Substituir arquivo
    -->
    <add key="falhaDuplicidade" value="3"/>


  </appSettings>
  <userSettings>
    <LeitorCodigoBarras.My.MySettings>
      <setting name="tamanhoLeitura" serializeAs="String">
        <value>0</value>
      </setting>
      <setting name="localArmazenamento" serializeAs="String">
        <value>C:\ScanImg\</value>
      </setting>
      <setting name="loteAtual" serializeAs="String">
        <value>0</value>
      </setting>
    </LeitorCodigoBarras.My.MySettings>
  </userSettings>
</configuration>
